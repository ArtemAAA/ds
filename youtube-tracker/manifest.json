{
  "manifest_version": 3,
  "name": "YouTube Tracker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
